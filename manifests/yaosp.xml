<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Manifest -->
  <remove-project name="AOSPA/manifest" />
  <project path="manifest" name="YAOSPA_manifest" remote="yaospa" groups="aospa" />

  <!-- Build repositories -->
  <remove-project name="AOSPA/android_build" />
  <project path="build" name="YAOSPA_build" remote="yaospa" groups="aospa" >
    <copyfile src="core/root.mk" dest="Makefile" />
  </project>

  <!-- Frameworks -->
  <remove-project name="AOSPA/android_frameworks_av" />
  <project path="frameworks/av" name="YAOSPA_frameworks_av" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_frameworks_base" />
  <project path="frameworks/base" name="YAOSPA_frameworks_base" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_frameworks_native" />
  <project path="frameworks/native" name="YAOSPA_frameworks_native" remote="yaospa" groups="aospa" />

  <remove-project name="platform/frameworks/support" />
  <project path="frameworks/support" name="YAOSPA_frameworks_support" remote="yaospa" groups="aospa" />

  <!-- Packages -->
  <remove-project name="platform/packages/apps/Calculator" />
  <project path="packages/apps/Calculator" name="YAOSPA_packages_apps_Calculator" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_packages_apps_PackageInstaller" />
  <project path="packages/apps/PackageInstaller" name="YAOSPA_packages_apps_PackageInstaller" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_packages_apps_Settings" />
  <project path="packages/apps/Settings" name="YAOSPA_packages_apps_Settings" remote="yaospa" groups="aospa" />

  <project path="packages/apps/SlimRecents" name="YAOSPA_packages_apps_SlimRecents" remote="yaospa" groups="aospa" />

  <remove-project name="platform/packages/providers/DownloadProvider" />
  <project path="packages/providers/DownloadProvider" name="YAOSPA_packages_providers_DownloadProvider" remote="yaospa" groups="aospa" />

  <!-- External -->
  <project path="external/busybox" name="YAOSPA_external_busybox" remote="yaospa" groups="aospa" />

  <!-- System -->
  <remove-project name="AOSPA/android_system_extras" />
  <project path="system/extras" name="YAOSPA_system_extras" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_system_sepolicy" />
  <project path="system/sepolicy" name="YAOSPA_system_sepolicy" remote="yaospa" groups="aospa" />

  <!-- Vendor -->
  <remove-project name="AOSPA/android_vendor_pa" />
  <project path="vendor/pa" name="YAOSPA_vendor_pa" remote="yaospa" groups="aospa" />

  <project path="vendor/yaosp" name="YAOSPA_vendor_yaosp" remote="yaospa" groups="aospa" />

  <!-- Telephony -->
  <remove-project name="AOSPA/android_packages_apps_Dialer" />
  <project path="packages/apps/Dialer" name="YAOSPA_packages_apps_Dialer" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_packages_services_Telecomm" />
  <project path="packages/services/Telecomm" name="YAOSPA_packages_services_Telecomm" remote="yaospa" groups="aospa" />

  <remove-project name="AOSPA/android_packages_services_Telephony" />
  <project path="packages/services/Telephony" name="YAOSPA_packages_services_Telephony" remote="yaospa" groups="aospa" />

  <!-- Obsolete Packages -->
  <remove-project name="AOSPA/android_packages_apps_ParanoidHub"/>

</manifest>
